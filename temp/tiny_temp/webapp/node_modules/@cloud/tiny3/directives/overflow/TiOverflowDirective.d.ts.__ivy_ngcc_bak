import { ElementRef, OnInit } from '@angular/core';
import { TiOverflowService } from '../../services/overflow/TiOverflowService';
import { TiPositionType } from '../../utils/Position';
/**
 * 超长情况下文本处理出...并tip提示
 *
 * <example-url>../tiny3demo/#/overflow/overflow-all</example-url>
 */
export declare class TiOverflowDirective implements OnInit {
    private ele;
    private overflowService;
    /**
     * 配置宿主元素支持文本过长提示功能，不需要配置属性值，存在该属性即可
     */
    tiOverflow: any;
    /**
     * 配置文本过长时显示的tip内容，默认为宿主元素文本
     */
    tiTipContent: string;
    /**
     * 配置文本过长时显示的tip位置
     */
    tiTipPosition: TiPositionType;
    /**
     * 配置文本过长时显示的tip的最大宽度。
     *
     * 3.0.9新增
     */
    tiTipMaxWidth: string;
    /**
     * @ignore
     * 决定tip水平方向位置的宿主元素配置
     */
    tiTipHostEleX: Element;
    private overflowRef;
    constructor(ele: ElementRef, overflowService: TiOverflowService);
    ngOnInit(): void;
    ngOnDestroy(): void;
}
