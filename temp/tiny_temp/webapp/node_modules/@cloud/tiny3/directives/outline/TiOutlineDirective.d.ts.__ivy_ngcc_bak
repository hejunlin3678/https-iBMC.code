import { ElementRef, Renderer2, NgZone, AfterViewInit } from '@angular/core';
/**
 * @ignore
 * 点击需要和聚焦区分开。当点击的时候，不需要有outline。
 * 10.1.4新增
 */
export declare class TiOutlineDirective implements AfterViewInit {
    private renderer;
    hostEle: ElementRef;
    private zone;
    constructor(renderer: Renderer2, hostEle: ElementRef, zone: NgZone);
    ngAfterViewInit(): void;
}
