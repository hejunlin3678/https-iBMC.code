import { ElementRef, EventEmitter, IterableDiffers, KeyValueDiffers, Renderer2, TemplateRef } from '@angular/core';
import { TiFormComponent } from '../base/TiBaseModule';
import { TiMenuComponent, TiMenuItem } from '../menu/TiMenuModule';
import { TiPopconfirmConfig } from '../../directives/popconfirm/TiPopconfirmDirective';
/**
 * 10.0.3/9.0.7版本新增该接口
 *
 * 在此之前版本可直接使用`TiMenuItem`接口
 */
export interface TiActionmenuItem extends TiMenuItem {
    /**
     * 气泡确认框配置，具体可参考tiPopconfirm指令
     *
     * 在actionmenu中，只有暴露在更多按钮外部，直接显示在页面中的菜单项，配置后会显示确认框
     */
    popConfig?: TiPopconfirmConfig;
}
/**
 * ActionMenu菜单按钮组件
 *
 * ActionMenu组件主要是一组操作按钮。当按钮数量太多导致预留空间大小无法显示所有按钮时，自动将超出部分的按钮放置在一个menu组件下拉中。
 *
 * 使用该组件，用户只需关注操作项配置，无需关注操作项显示不足的情况。
 *
 * 注意：ExpressionChangedAfterItHasBeenCheckedError: Expression has changed after it was checked.（本组件在调试环境报此错误，不必理会。生产环境不报错，功能也不受影响。）
 *
 * <example-url>../tiny3demo/#/actionmenu/actionmenu-all</example-url>
 */
export declare class TiActionmenuComponent extends TiFormComponent {
    protected hostRef: ElementRef;
    protected renderer: Renderer2;
    private iterableDiffers;
    private keyValueDiffers;
    /**
     * 选项数据集
     */
    items: Array<TiActionmenuItem>;
    /**
     * 下拉面板最大宽度，超过时换行，对所有子菜单都生效。
     */
    panelMaxWidth: string;
    /**
     * 下拉面板最大高度度，超过时出现竖向滚动条，对所有子菜单都生效。
     */
    panelMaxHeight: string;
    /**
     * 数据中要显示字符串的key
     */
    labelKey: string;
    /**
     * 用户点击选中菜单项事件
     */
    readonly select: EventEmitter<TiActionmenuItem>;
    /**
     * 外部容器宽度
     */
    maxWidth: string;
    /**
     * 各item的间距
     */
    space: string;
    /**
     * 各item之间是否展示分割线
     *
     * 3.1.3新增
     */
    showDivider: boolean;
    /**
     * 最大显示个数, 包含下拉菜单
     */
    maxShowNum: number;
    /**
     * 下拉菜单上方显示文本
     *
     * 缺省值 : 更多/more（国际化）
     */
    moreText: string;
    /**
     * 只显示更多项下拉菜单时，下拉菜单上方显示文本
     *
     * 缺省值 : 操作/operation（国际化）
     */
    menuText: string;
    /**
     * 常常绑定表格本行数据。
     */
    data: any;
    /**
     * 表格本行data数据，转化为items数据。类型：(data: any) => Array<MenuItem>
     */
    dataToItemsFn: (data: any) => Array<TiActionmenuItem>;
    /**
     * @ignore
     * 用户写的自定义模板
     */
    firstTemplate: TemplateRef<any>;
    itemTemplate: TemplateRef<any>;
    tipTemplate: TemplateRef<any>;
    /**
     * @ignore
     */
    menuCom: TiMenuComponent;
    /**
     * @ignore
     * 下拉菜单绑定的选项数据
     */
    panelItems: Array<TiActionmenuItem>;
    /**
     * @ignore
     * 各item之间间隔的一半距离
     */
    halfSpace: string;
    private itemsDiffer;
    private dataDiffer;
    private needInitItems;
    constructor(hostRef: ElementRef, renderer: Renderer2, iterableDiffers: IterableDiffers, keyValueDiffers: KeyValueDiffers);
    private static isEqualOffsetTop;
    /**
     * @ignore
     *  选中菜单内选项
     * @param item 菜单项数据
     */
    onSelect(item: TiActionmenuItem): void;
    ngOnInit(): void;
    ngDoCheck(): void;
    /**
     * 兼容旧版：
     * 10.0.3 版本之前只能内嵌一个模板，无命名。
     * 新版可以内嵌两个模板，示例书写要求都命名（#item，#tip）。
     * 但需要兼容旧版无命名测试用例。
     */
    ngAfterContentInit(): void;
    ngAfterViewChecked(): void;
    /**
     * 计算那些item应该显示在外部，移除不该显示的item，调整pannel上显示的item项，调整menu显示的文字。
     */
    private initItems;
    /**
     * 设置显示/隐藏样式，比ngIf更及时生效。
     * @param elem HTML元素
     * @param isShow 是否显示
     */
    private setDisplay;
    /**
     * @ignore
     * ngFor遍历的 trackBy函数，防止数据更新导致所有DOM重新渲染。TODO：这里是否该使用trackBy?
     * @param index 索引
     * @param item 数据
     * @returns 索引
     */
    trackByFn(index: any, item: any): any;
}
