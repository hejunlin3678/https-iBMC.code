import { ElementRef, Renderer2 } from '@angular/core';
import { TiFileItem } from '../../services/upload/TiFileInterface';
import { TiUploadService } from '../../services/upload/TiUploadService';
import { TiOverflowService } from '../../services/overflow/TiOverflowService';
import { TiUploadbaseComponent } from './TiUploadbaseComponent';
/**
 * 该组件用于实现已设计好的交互完整的文件上传组件，包含两种样式：
 *
 * 1.带输入框的样式(type为inputField)
 *
 * 2.按钮样式(type为button)
 *
 * 如果以上两种样式不满足业务场景，可使用自定义方式实现，具体实现请参考[tiFileSelect]{@link ../directives/TiFileSelectDirective.html}
 *
 * <example-url>../tiny3demo/#/upload/upload-all</example-url>
 */
export declare class TiUploadComponent extends TiUploadbaseComponent {
    private uploaderService;
    private tiOverflow;
    constructor(uploaderService: TiUploadService, hostEle: ElementRef, renderer: Renderer2, tiOverflow: TiOverflowService);
    /**
     * 上传样式类型
     */
    type: 'inputField' | 'button';
    /**
     * 按钮的配置
     *
     * type为button形式时,为文件选择按钮文字；
     *
     * type为inputField时,为上传按钮文字
     */
    buttonText: string;
    /**
     * 选择输入框宽度设置，只适用于inputField类型，数据类型同css width属性设置
     */
    inputFieldWidth: string;
    /**
     * 是否显示提交按钮，只适用于inputField类型
     *
     * 默认值与autoUpload值相关，当autoUpload为true时，不显示；反之则显示
     */
    showSubmitButton: boolean;
    /**
     * 输入提示，只适用于inputField类型
     */
    placeholder: string;
    /**
     * 是否自动聚焦，只适用于inputField类型
     */
    autofocus: boolean;
    /**
     * @ignore
     */
    fileInput: any;
    /**
     * @ignore 多文件上传的状态提示信息
     */
    stateInfo: string;
    /**
     * @ignore 是否定义beforeRemoveItems事件
     */
    isRemove: boolean;
    /**
     * @ignore 点击上传按钮触发事件
     */
    onSelectClick(): void;
    /**
     * @ignore
     */
    ngOnInit(): void;
    ngAfterViewInit(): void;
    /**
     * 聚焦方法，只适用于inputField类型
     */
    focus(): void;
    /**
     * 失焦方法，只适用于inputField类型
     */
    blur(): void;
    /**
     * @ignore
     */
    onItemMouseenter(item: TiFileItem, event: any): void;
    /**
     * @ignore
     */
    onItemMouseleave(item: TiFileItem): void;
    /**
     * @ignore
     */
    getUploadState(): string;
    /**
     * 初始设置输入框内提示文本
     * 四种场景：自动单文件上传，自动多文件上传，手动单文件上传，手动多文件上传
     */
    private setPlaceholder;
}
