import { SimpleChanges, TemplateRef } from '@angular/core';
import { TiDateBaseComponent } from '../datebase/TiDateBaseModule';
/**
 * Date日期组件
 *
 * Date组件提供了一种方便的显示和设置日期的方式
 *
 * <example-url>../tiny3demo/#/date/date-all</example-url>
 */
export declare class TiDateComponent extends TiDateBaseComponent {
    /**
     * @ignore
     * 日期显示格式: date组件的format为string类型
     */
    format: string;
    /**
     * @ignore
     * 标记date/datetime的类型
     */
    isDatetime: boolean;
    /**
     * @ignore
     * 用于标记是不是range
     */
    isRange: boolean;
    /**
     * @ignore
     * placeholder提示文本
     */
    placeholder: string;
    /**
     * @ignore
     */
    customizeTemplate: TemplateRef<any>;
    ngOnChanges(changes: SimpleChanges): void;
    /**
     * @ignore
     * model值变化时的回调
     */
    ngOnModelChange(): void;
    /**
     * @ignore
     */
    setInputValue(value: Date): void;
    /**
     * @ignore
     */
    onKeydownFn(event: KeyboardEvent, val: any, pos: string): void;
    /**
     * @ignore
     * 配置时间日期面板接口
     */
    setPickerDate(): void;
    /**
     * @ignore
     * 根据model值和format接口，格式化显示时间日期
     */
    formatValue(): void;
    /**
     * @ignore
     * 判断是不是合法的model
     */
    isValidValue(value: Date): boolean;
}
