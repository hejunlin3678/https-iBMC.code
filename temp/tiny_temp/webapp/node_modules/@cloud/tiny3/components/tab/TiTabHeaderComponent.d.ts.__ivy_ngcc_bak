import { AfterContentInit, AfterViewChecked, ElementRef } from '@angular/core';
import { TiTabComponent } from './TiTabComponent';
/**
 * TiTabHeaderComponent 是单个页签的标题头组件，其包裹的元素会作为当前页签的标题头
 *
 */
export declare class TiTabHeaderComponent implements AfterContentInit, AfterViewChecked {
    private tab;
    private elementRef;
    private oldHeaderWidth;
    constructor(tab: TiTabComponent, elementRef: ElementRef);
    ngAfterContentInit(): void;
    ngAfterViewChecked(): void;
}
