import { ElementRef, EventEmitter, OnChanges, OnDestroy, OnInit, Renderer2, SimpleChanges, TemplateRef } from '@angular/core';
import { TiTabsComponent } from './TiTabsComponent';
import { TiBaseComponent } from '../base/TiBaseModule';
/**
 * TiTabComponent 是单个页签组件，包含了页签标题头指令TiTabHeader 和 该页签对应的内容部分
 *
 */
export declare class TiTabComponent extends TiBaseComponent implements OnInit, OnDestroy, OnChanges {
    private elementRef;
    private renderer2;
    tabset: TiTabsComponent;
    /**
     * 设置当前页签标题头显示的文本
     */
    header: string;
    /**
     * 设置当前页签是否禁用置灰
     */
    disabled: boolean;
    /**
     * 设置当前页签是否处于激活状态。
     *
     * 此接口是双向绑定的，设置时中括号“[]”和圆括号“()”要同时使用
     */
    active: boolean;
    /**
     * 当前页签的激活状态发生改变时向外进行事件通知,传递的参数为 boolean 类型，表示当前页签是否是激活状态
     */
    readonly activeChange: EventEmitter<any>;
    /**
     * @ignore
     * 获取到用户自定义的模板
     */
    contentTemplate: TemplateRef<any>;
    /**
     * @ignore
     * 当前页签标题头显示的node节点
     */
    headNode: any;
    /**
     * @ignore
     * 记录此页签的内容区是否已经加载过
     */
    loaded: boolean;
    constructor(elementRef: ElementRef, renderer2: Renderer2, tabset: TiTabsComponent);
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    ngOnDestroy(): void;
    private createHeadNode;
}
