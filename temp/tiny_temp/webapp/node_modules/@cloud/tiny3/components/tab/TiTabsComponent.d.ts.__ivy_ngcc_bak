import { AfterViewChecked, AfterViewInit, ElementRef, NgZone, OnInit, OnDestroy, Renderer2 } from '@angular/core';
import { TiTabComponent } from './TiTabComponent';
import { TiBaseComponent } from '../base/TiBaseModule';
import { TiRenderer } from '../../services/renderer/TiRenderer';
import { TiDroplistComponent } from '../droplist/TiDroplistModule';
import { TiPositionType } from '../../utils/Position';
/**
 * TiTabs组件属于布局组件，通过该组件产品可以实现不同视图的切换功能。
 *
 * 其内部包含2个组件 TiTab 和 TiTabHeader
 *
 * <example-url>../tiny3demo/#/tab/tab-all</example-url>
 */
export declare class TiTabsComponent extends TiBaseComponent implements AfterViewInit, AfterViewChecked, OnInit, OnDestroy {
    private elementRef;
    private renderer2;
    tiRenderer: TiRenderer;
    private zone;
    private document;
    /**
     * 1."justified"(默认): 下拉框的宽度与输入框的宽度保持一致；
     *
     * 2."auto": 下拉框的宽度根据下拉项的内容自动撑开；
     *
     * 3.表示宽度的字符串: 设置固定的下拉框宽度(不小于输入框的宽度)。例如："200px"。
     *
     * 10.0.1新增
     */
    panelWidth: string;
    /**
     * 面板对齐方式
     *
     * 10.0.1新增
     *
     */
    panelAlign: 'left' | 'right';
    /**
     * 面板最大高度
     *
     * 10.0.1新增
     *
     */
    panelMaxHeight: string;
    /**
     * 指定下拉项超长文本提示方向
     *
     * 10.0.1新增
     *
     */
    tipPosition: TiPositionType;
    /**
     * @ignore
     */
    slider: ElementRef;
    /**
     * @ignore
     */
    tabContentContainer: ElementRef;
    /**
     * @ignore
     */
    moreRef: ElementRef;
    /**
     * @ignore
     */
    moreIconRef: ElementRef;
    /**
     * @ignore
     */
    tabsListRef: ElementRef;
    /**
     * @ignore
     */
    tabsHeaderRef: ElementRef;
    /**
     * @ignore
     */
    tabsContainerRef: ElementRef;
    /**
     * @ignore
     */
    dropListComp: TiDroplistComponent;
    /**
     * @ignore
     */
    labelKey: string;
    /**
     * @ignore
     * 下拉项
     */
    options: Array<any>;
    /**
     * @ignore
     */
    onlistenClick: () => void;
    /**
     * @ignore
     */
    windowResizeListener: () => void;
    /**
     * @ignore
     * TiTabs中包裹的所有的 TiTabComponent 集合
     */
    tabs: Array<TiTabComponent>;
    /**
     * @ignore
     */
    selectedTab: TiTabComponent;
    private isNotVisibleInit;
    /**
     * @ignore
     * TiTabComponent.ts中使用
     */
    destroyed: boolean;
    /**
     * @ignore
     * 是否显示滑动条slider（一级页签显示，二级/暗色页签不显示）
     * TiTabHeaderComponent.ts中使用
     */
    hasSlider: boolean;
    /**
     * @ignore
     * TiTabHeaderComponent.ts中使用
     */
    headerWidthChange: boolean;
    private resolvedPromise;
    /**
     * @ignore
     * 下拉选项变更时触发
     */
    onDroplistChange($event: any): void;
    /**
     * @ignore
     * 点击下拉按钮打开下拉面板
     */
    showMorePanel(event: MouseEvent): void;
    constructor(elementRef: ElementRef, renderer2: Renderer2, tiRenderer: TiRenderer, zone: NgZone, document: any);
    ngOnInit(): void;
    ngAfterViewInit(): void;
    ngAfterViewChecked(): void;
    ngOnDestroy(): void;
    /**
     * @ignore
     */
    setMoreStyle(): void;
    /**
     * @ignore
     * TiTabComponent.ts 中调用
     */
    addTab(tab: TiTabComponent): void;
    /**
     * @ignore
     * TiTabComponent.ts 中调用
     */
    removeTab(tab: TiTabComponent): void;
    /**
     * @ignore
     * TiTabComponent.ts 中调用
     */
    activeTab(selectedTab: TiTabComponent, enableAnimate: boolean): void;
    private deActiveOthers;
    /**
     * @ignore
     */
    click(tab: TiTabComponent): void;
    private getNewActiveIndex;
    private setTabStyle;
    private setTabScroll;
    private setActiveValue;
    /**
     * @ignore
     */
    trackByFn(index: number, item: any): Node;
    private isOverflow;
}
