import { SimpleChanges } from '@angular/core';
import { TiDateBaseComponent, TiDateValue } from '../datebase/TiDateBaseModule';
/**
 * DateRange日期范围组件
 *
 * DateRange组件提供了一种方便的显示和设置日期范围的方式
 *
 * <example-url>../tiny3demo/#/daterange/daterange-all</example-url>
 */
export declare class TiDateRangeComponent extends TiDateBaseComponent {
    /**
     * @ignore
     * 日期显示格式: date组件的format为string类型
     */
    format: string;
    /**
     * @ignore
     * 标记date/datetime的类型
     */
    isDatetime: boolean;
    /**
     * @ignore
     * 用于标记是不是range
     */
    isRange: boolean;
    /**
     * @ignore
     * 保存model值
     */
    oldModel: TiDateValue;
    /**
     * @ignore
     * placeholder提示文本
     */
    placeholder: string;
    ngOnChanges(changes: SimpleChanges): void;
    ngDoCheck(): void;
    /**
     * @ignore
     * 将value转换成format接口格式的字符串
     */
    formatValue(): void;
    /**
     * @ignore
     */
    onKeydownFn(event: KeyboardEvent, pos: string): void;
    /**
     * @ignore
     */
    dateEditBlur(pos: string): void;
    /**
     * @ignore
     * 确认按钮
     */
    onOkClick(): void;
    /**
     * @ignore
     * 是否为禁用日期
     */
    isDisabledDays(value: Date): boolean;
    /**
     * @ignore
     * model值的合法性判断
     */
    isValidValue(value: TiDateValue): boolean;
    private isValidRange;
    /**
     * @ignore
     * 设置确认按钮的状态
     */
    setOkBtnState(): void;
    /**
     * @ignore
     * 配置时间日期面板接口
     */
    setPickerDate(): void;
}
