import { SimpleChanges } from '@angular/core';
import { TiBaseComponent } from '../base/TiBaseModule';
/**
 * 进度条组件
 *
 * <example-url>../tiny3demo/#/progressbar/progressbar-all</example-url>
 */
export declare class TiProgressbarComponent extends TiBaseComponent {
    /**
     *  设置当前值，当前值/最大值即为进度值
     */
    value: number;
    /**
     *  设置最大值，当前值/最大值即为进度值
     */
    max: number;
    /**
     *  设置进度条在不同当前值下对应的样式，包括动画效果
     */
    progressClass: string;
    /**
     *  设置进度条是否开启动画效果
     */
    animation: boolean;
    /**
     * @ignore
     */
    percent: number;
    private static verifyNum;
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    /**
     * @description 处理用户输入的改变
     * @param: changes 改变的对象
     * @param: changeKey 那个属性改变
     */
    private setChanges;
    private calcPercentage;
}
