import { EventEmitter } from '@angular/core';
import { TiBaseComponent } from '../base/TiBaseModule';
import { TiPositionType } from '../../utils/Position';
/**
 * Tag标签组件，支持显示自定义内容，和删除事件通知。
 *
 * <example-url>../tiny3demo/#/tag/tag-all</example-url>
 */
export declare class TiTagComponent extends TiBaseComponent {
    /**
     * @ignore
     * 样式：有方形和箭头形两种样式，后面可能废弃箭头样式
     */
    /**
     * 是否禁用
     */
    disabled: boolean;
    /**
     * 选中项文本超出时tip展开方向
     *
     * 9.0.3新增
     */
    selectedTipPosition: TiPositionType;
    /**
     * @ignore
     * 最大宽度，TpSearchbox 使用
     */
    maxWidth: string;
    /**
     * 删除Tag时的通知事件
     */
    readonly delete: EventEmitter<any>;
    /**
     * @ignore
     * @param $event
     */
    onClickDelete($event: MouseEvent): void;
    /**
     * 删除Tag标签element
     */
    remove(): void;
}
