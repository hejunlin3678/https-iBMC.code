import { AfterContentInit, ElementRef } from '@angular/core';
import { TiItemComponent } from './TiItemComponent';
/**
 * 本组件嵌在 tiItem 中使用，包裹着单个表单条目中 label显示的dom片段
 */
export declare class TiItemLabelComponent implements AfterContentInit {
    private item;
    private elementRef;
    constructor(item: TiItemComponent, elementRef: ElementRef);
    ngAfterContentInit(): void;
}
