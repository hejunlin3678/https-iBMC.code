    protected getListOptions(): Array<TiMenuItem>;
    hasBorder(item: TiMenuItem, items: Array<TiMenuItem>): boolean;
    onMouseenterItem(event: MouseEvent, option: any): void;
    ngOnChanges(): void;
}
