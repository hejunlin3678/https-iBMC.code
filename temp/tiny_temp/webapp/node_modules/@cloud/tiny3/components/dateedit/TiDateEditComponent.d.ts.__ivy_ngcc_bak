import { ElementRef, Renderer2, SimpleChanges } from '@angular/core';
import { TiFormComponent } from '../base/TiBaseModule';
/**
 * @ignore
 */
export declare class TiDateEditComponent extends TiFormComponent {
    protected hostRef: ElementRef;
    protected render: Renderer2;
    format: string;
    min: Date;
    max: Date;
    disabled: boolean;
    disabledDays: Array<any>;
    inputRef: ElementRef;
    /**
     * @ignore
     */
    display: string;
    protected oldMinvalue: Date;
    protected oldMaxvalue: Date;
    private oldModel;
    private oldInputvalue;
    inputValue: string;
    placehoderText: string;
    MAX: Date;
    MIN: Date;
    constructor(hostRef: ElementRef, render: Renderer2);
    writeValue(value: Date): void;
    ngOnModelChange(): void;
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    /**
     * 不同场景下设置日期输入框placehoder文本
     */
    private setPlacehoderText;
    onFocus(): void;
    onBlur(): void;
    onKeydown(event: KeyboardEvent): void;
    /**
     * @ignore
     */
    handleInputValue(): void;
    isValidValue(value: Date): boolean;
    /**
     * @ignore
     */
    filterEmptyFn(value: any): any;
    private validateFormat;
    private validateMinAndMax;
    formatValue(): void;
    private getValidModel;
    /**
     * @ignore
     * 是否为禁用日期
     */
    isDisabledDays(value: Date): boolean;
}
