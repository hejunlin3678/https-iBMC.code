import { DoCheck, ElementRef, OnInit } from '@angular/core';
import { TiTableComponent } from './TiTableComponent';
/**
 * TiHeadSort 表头排序(上下箭头)组件
 *
 * 嵌在表头的th中使用，点击该组件时表格数据按该列的排序规则进行升序/降序排序
 *
 * 组件有默认的排序规则，也可支持自定义排序规则
 */
export declare class TiHeadSortComponent implements OnInit, DoCheck {
    private elementRef;
    /**
     * 指定当前列进行排序的数据属性
     *
     * 例如：根据表格数据中的"firstName"属性值进行排序，则需将该接口的值设置为"firstName"
     */
    sortKey: string;
    /**
     * 指定初始时在对应 sortKey下是否为升序排序。默认值为null, 表示无序(不升序也不降序)
     */
    asc: boolean;
    /**
     * 指定排序对比规则，其参数有三个，依次为：
     *
     * a: 第一个对比值
     *
     * b: 第二个对比值
     *
     * sortKey: 当前数据排序依据的属性
     */
    compareFn: (a: any, b: any, sortKey?: string) => number;
    /**
     * @ignore
     */
    clickNum: number;
    private hostElement;
    /**
     * @ignore
     */
    table: TiTableComponent;
    constructor(elementRef: ElementRef, table: TiTableComponent);
    ngOnInit(): void;
    ngDoCheck(): void;
    /**
     * @ignore
     */
    onClick(): void;
    private sort;
}
