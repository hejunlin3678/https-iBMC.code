import { DoCheck, ElementRef, EventEmitter, OnInit, Renderer2 } from '@angular/core';
import { TiTableComponent, TiTableRowData } from './TiTableComponent';
/**
 * TiDetailsIcon 详情展开图标组件
 *
 * ti-details-icon 用来显示详情展开图标，其嵌在详情展开列的 td 标签中；
 * 点击该图标，对应详情行在展开/收起两个状态之间切换。
 */
export declare class TiDetailsIconComponent implements OnInit, DoCheck {
    private table;
    private elementRef;
    private renderer;
    /**
     * @ignore
     * 表格详情中，非详情行标记样式类
     */
    static TABLE_ClASS_DETAIL_BASE: string;
    /**
     * 当前行数据
     */
    row: TiTableRowData;
    /**
     * 当前行索引值，即 ngFor的对应的index
     */
    index: number;
    /**
     * 点击详情展开图标时触发此接口函数，其中showDetails属性控制其是否展开；
     *
     * 一旦使用此接口，就由开发者决定其是否要展开或收起。
     */
    readonly beforeToggle: EventEmitter<TiTableRowData>;
    private oldShowDetails;
    private element;
    constructor(table: TiTableComponent, elementRef: ElementRef, renderer: Renderer2);
    ngOnInit(): void;
    ngDoCheck(): void;
    /**
     * @ignore
     */
    onClick(): void;
}
