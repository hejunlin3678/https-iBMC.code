import { RendererFactory2 } from '@angular/core';
import { TiRenderer } from '../../services/renderer/TiRenderer';
import { ResizableOpts, TiColsResizableDirective } from './TiColsResizableDirective';
import { TiTableComponent } from './TiTableComponent';
/**
 * @ignore
 * 提供表头锁定功能
 */
export declare class TiTableFixedHeadService {
    private tiRenderer;
    private renderer;
    constructor(rendererFactory: RendererFactory2, tiRenderer: TiRenderer);
    getTheadContainer(hostEle: any): any;
    getTbodyContainer(theadContainer: any): any;
    processOverflowY(theadContainer: any, tbodyContainer: any, tableCom: TiTableComponent): void;
    removeTbodyContainerBorderBottom(tbodyContainer: any, displayedData: Array<Object>): void;
    overflowedResult(tbodyContainer: any, direction: string, isNum?: boolean): any;
    processYScrollStateChange(colsResizableDire: TiColsResizableDirective): void;
    private getMaxWidthTh;
    handleYOverflowedWithX(resizableOpts: ResizableOpts, tbodyContainer: any): void;
    handleYNotOverflowedWithX(resizableOpts: ResizableOpts, tbodyContainer: any): void;
    getScrollBarWidth(direction: string): number;
    numRound(num: number, decimal: number): number;
}
