import { TiModalService } from '@cloud/tiny3';
/**
 * 二次认证组件
 *
 * <example-url>../tinyplus3demo/#/identityauth/identityauth-all</example-url>
 */
export declare class TpIdentityauthService {
    private tiModal;
    constructor(tiModal: TiModalService);
    /**
     * @ignore
     */
    camel: any;
    /**
     * @ignore
     */
    mask: any;
    /**
     * @ignore
     */
    msgService: any;
    /**
     * @ignore
     */
    userId: string;
    /**
     * @ignore
     */
    serviceEndpointList: Array<any>;
    private appWebPath;
    /**
     * @ignore
     */
    successFn: () => void;
    private queryIsOpen;
    private checkCerifyCode;
    private identityAuth;
    /**
     * 开启二次认证
     *
     * @param successFn 二次认证通过或不需要认证时的回调函数
     *
     * @param config 组件本地测试需要的参数，开发者实际场景中不需要
     */
    open(successFn: () => void, config?: any): void;
}
