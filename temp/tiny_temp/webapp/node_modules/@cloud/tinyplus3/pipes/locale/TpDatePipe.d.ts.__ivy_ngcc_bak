import { PipeTransform } from '@angular/core';
/**
 * 日期格式化
 *
 * 默认按照局点配置格式
 *
 * <example-url>../tinyplus3demo/#/locale/locale-all</example-url>
 */
export declare class TpDatePipe implements PipeTransform {
    /**
     * 日期转换
     * @param value 日期
     * @param format 显示种类
     * @returns 转换结果
     */
    transform(value: string | number | Date, format?: 'date' | 'time' | 'dateTime' | 'dateTimeZone'): string;
}
