import { ElementRef, Renderer2, TemplateRef } from '@angular/core';
import { TiBaseComponent, TiModalService } from '@cloud/tiny3';
export declare class TpSearchboxHelpComponent extends TiBaseComponent {
    private tiModal;
    /**
     * @ignore
     * 默认字段
     */
    defaultField: string;
    /**
     * @ignore
     * 示例1选中项
     */
    tags1: Array<any>;
    /**
     * @ignore
     * 示例二选中项
     */
    tags2: Array<any>;
    /**
     * @ignore
     * 示例二选中项
     */
    tags3: Array<any>;
    /**
     * @ignore
     * 输入项
     */
    items: Array<any>;
    /**
     * @ignore
     * 弹窗模板
     */
    helpModalTemplateRef: TemplateRef<any>;
    constructor(hostRef: ElementRef, render: Renderer2, tiModal: TiModalService);
    ngOnInit(): void;
    /**
     * 展示弹窗
     */
    show(): void;
}
