import { ElementRef, EventEmitter, NgZone, QueryList, Renderer2, SimpleChanges } from '@angular/core';
import { TiBaseComponent } from '@cloud/tiny3';
import { TpSwipercardComponent } from './TpSwipercardComponent';
/**
 * swiper组件每个展示块为一个card，点击左右按钮可以实现card切换功能
 *
 * <example-url>../tinyplus3demo/#/swiper/swiper-all</example-url>
 */
export declare class TpSwiperComponent extends TiBaseComponent {
    private elementRef;
    private renderer2;
    private zone;
    constructor(elementRef: ElementRef, renderer2: Renderer2, zone: NgZone);
    /**
     * 指定当前激活项
     */
    activeIndex: number;
    /**
     * 10.0.2新增
     *
     * 是否自动播放
     */
    autoplay: boolean;
    /**
     * 10.0.2新增
     *
     * 自动播放时，默认的播放（翻页）速度
     */
    autoplaySpeed: number;
    /**
     * 10.1.2新增
     *
     * 每页展示卡片数量
     */
    showCardNum: number;
    /**
     * 10.1.2新增
     *
     * 多图轮播卡片间距，8-12px，默认为8px（规范建议使用偶数值，即8px，10px，12px）
     */
    cardGap: string;
    /**
     * 10.1.2新增
     *
     * 指示器位置，三个属性值，‘below’容器外部，‘bottom’容器内部底部，‘none’不显示
     */
    indicatorPosition: 'bottom' | 'below' | 'none';
    /**
     * 10.1.2新增
     *
     * 是否无限循环
     */
    loop: boolean;
    /**
     * 当前卡片切换触发的事件
     */
    readonly activeIndexChange: EventEmitter<any>;
    /**
     * @ignore
     */
    cards: ElementRef;
    /**
     * @ignore
     */
    swipercardComs: QueryList<TpSwipercardComponent>;
    /**
     * @ignore
     */
    wrapperEle: ElementRef;
    /**
    /**
     * @ignore
     * 卡片总页数
     */
    totalPage: number;
    /**
     * @ignore
     * 当前页， 按照索引值从0开始
     */
    currentPage: number;
    /**
     * @ignore
     * 面板指示点的集合
     */
    swiperBullets: Array<any>;
    private transitionSpeed;
    private autoplayId;
    private cardNum;
    private pageWidth;
    private swiperCardWidth;
    private cardSpace;
    private totalWidth;
    private unlistenResize;
    ngOnInit(): void;
    ngAfterContentChecked(): void;
    ngAfterViewInit(): void;
    ngAfterViewChecked(): void;
    ngOnChanges(changes: SimpleChanges): void;
    ngOnDestroy(): void;
    /**
     * @ignore
     * 点击左侧按钮触发事件
     */
    onClickPrev(): void;
    /**
     * @ignore
     * 点击右侧按钮触发事件
     */
    onClickNext(): void;
    /**
     * @ignore
     * 点击指示器触发事件
     */
    onClickDot(index: number): void;
    /**
     * @ignore
     * 点击左右侧按钮、面板指示板触发事件
     */
    private activeCard;
    private setCardNum;
    private setCardsWidth;
    private setLeftPosition;
    private setPosition;
    private adjustPosition;
    private calculatpaging;
    /**
     * 计算卡片宽度
     */
    private calculatecardWidth;
    /**
     * 计算总页数
     */
    private calculateTotalPage;
    /**
     *  计算当前页
     */
    private calculateCurrentPage;
}
