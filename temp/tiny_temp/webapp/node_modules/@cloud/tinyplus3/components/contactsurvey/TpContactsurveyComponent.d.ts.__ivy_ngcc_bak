import { ElementRef, OnInit, Renderer2 } from '@angular/core';
import { TiBaseComponent } from '@cloud/tiny3';
export interface TpQuestionInfo {
    /**
     * 问题Id
     *
     */
    questionId: string;
    /**
     * 问题的类型，有radio和essay、两种类型
     *
     */
    type: 'radio' | 'essay';
    /**
     * 问题是否呈现
     */
    show: boolean;
    /**
     * 问题是否是必选
     *
     */
    require: boolean;
    /**
     * 问题的名称
     *
     */
    title: string;
}
export interface TpSurveyOptions {
    /**
     * 服务Id，各个服务需要唯一
     *
     */
    serviceId: string;
    /**
     * 触点Id，一个服务内的触点Id需要保证唯一
     *
     */
    contactId: string;
    /**
     * 触点名称，用于拼接问题
     *
     */
    contactName: string;
}
/**
 * 满意度调查组件
 *
 * 10.0.3版本新增
 *
 * <example-url>../tinyplus3demo/#/contactsurvey/contactsurvey-all</example-url>
 */
export declare class TpContactsurveyComponent extends TiBaseComponent implements OnInit {
    private elementRef;
    private renderer2;
    /**
     * 常量配置(90天对应的毫秒数)
     */
    private static readonly MILLISECONDS_PER_MONTH;
    /**
     * 组件传入数据
     *
     */
    options: TpSurveyOptions;
    /**
     * @ignore
     * 是否显示满意度调查组件，默认不显示。符合某些条件才会展示出来。
     */
    showContactsurvey: boolean;
    /**
     * @ignore
     * 控制提交按钮是否禁用，默认为禁用
     */
    submitDisabled: boolean;
    /**
     * @ignore
     * 国际化词条对象
     */
    tpContactsurvey: any;
    /**
     * @ignore
     * 评分值
     */
    scoreValue: number;
    /**
     * @ignore
     * 文本框组件输入值
     */
    textareaValue: string;
    /**
     * @ignore
     * 模板中展示的题目
     */
    displayData: Array<TpQuestionInfo>;
    /**
     * @ignore
     * 用户反馈的答案
     */
    submitAnswer: Array<any>;
    /**
     * 类生产环境的请求数据
     */
    private devConfig;
    /**
     * 现网环境的请求数据
     */
    private prodConfig;
    /**
     * 用户ID，作为用户的唯一标识
     */
    private userId;
    /**
     * 默认为类生产环境配置
     */
    config: any;
    constructor(elementRef: ElementRef, renderer2: Renderer2);
    ngOnInit(): void;
    /**
     * @ignore
     *  叉号图标点击事件
     */
    onClickClose(): void;
    /**
     * @ignore
     * 调查结果提交事件
     */
    onClickSubmit(): void;
    /**
     * @ignore
     * 点击评分组件
     */
    changeScore(score: number): void;
    /**
     * @ignore
     * 文本框失焦事件
     */
    onBlur(value: string): void;
    /**
     * @ignore
     * 初始化答案对象，用于给后台下发
     */
    initAnswerArrray(): void;
}
