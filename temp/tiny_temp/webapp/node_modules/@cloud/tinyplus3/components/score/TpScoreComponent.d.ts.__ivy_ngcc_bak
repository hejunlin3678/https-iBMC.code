import { ElementRef, Renderer2 } from '@angular/core';
import { TiFormComponent } from '@cloud/tiny3';
/**
 * NPS评分组件
 *
 * <example-url>../tinyplus3demo/#/score/score-all</example-url>
 */
export declare class TpScoreComponent extends TiFormComponent {
    protected hostRef: ElementRef;
    protected renderer: Renderer2;
    /**
     * 设置最小评分值文本描述
     */
    minText: string;
    /**
     * 设置最大评分值文本描述
     */
    maxText: string;
    /**
     * @ignore
     * 定义含有11项成员的数组
     */
    scoreArray: Array<any>;
    constructor(hostRef: ElementRef, renderer: Renderer2);
    /**
     * @ignore
     * 选择评分时，触发事件
     */
    onClick(value: number): void;
}
