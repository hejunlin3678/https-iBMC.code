import { EventEmitter } from '@angular/core';
import { TiBaseComponent } from '@cloud/tiny3';
/**
 *  特殊卡片组件，添加卡片组件
 *
 * <example-url>../tinyplus3demo/#/card/card-all</example-url>
 */
export declare class TpCardAddComponent extends TiBaseComponent {
    /**
     * 禁用状态
     */
    disabled: boolean;
    /**
     * 点击事件
     */
    readonly add: EventEmitter<any>;
    /**
     * @ignore
     * 加号点击回调
     */
    onClick($event: Event): void;
}
