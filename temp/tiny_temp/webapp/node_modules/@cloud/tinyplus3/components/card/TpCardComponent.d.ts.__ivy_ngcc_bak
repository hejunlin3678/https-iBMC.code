import { TiBaseComponent } from '@cloud/tiny3';
/**
 * 卡片组件
 *
 * <example-url>../tinyplus3demo/#/card/card-all</example-url>
 */
export declare class TpCardComponent extends TiBaseComponent {
    /**
     * 卡片禁用状态
     */
    disabled: boolean;
    /**
     * 卡片激活选中状态
     */
    active: boolean;
}
