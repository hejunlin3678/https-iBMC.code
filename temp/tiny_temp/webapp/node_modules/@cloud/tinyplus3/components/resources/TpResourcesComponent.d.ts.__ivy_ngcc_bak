import { EventEmitter } from '@angular/core';
import { TpResourceItem } from './TpResourceItemComponent';
import { TiBaseComponent } from '@cloud/tiny3';
/**
 * 资源列表 resources
 *
 * <example-url>../tinyplus3demo/#/resources/resources-all</example-url>
 */
export declare class TpResourcesComponent extends TiBaseComponent {
    /**
     * 资源列表标题
     */
    title: string;
    /**
     * 列表对象主接口；用于列表中每个内容中参数的传递。
     */
    items: Array<TpResourceItem>;
    /**
     * 选中某项资源时，触发事件
     */
    readonly select: EventEmitter<TpResourceItem>;
    /**
     * 选中某项资源异常数时，触发事件
     */
    readonly errorSelect: EventEmitter<TpResourceItem>;
    /**
     * @ignore
     */
    onClick(item: TpResourceItem): void;
    /**
     * @ignore
     */
    onErrorSelect(item: TpResourceItem): void;
}
