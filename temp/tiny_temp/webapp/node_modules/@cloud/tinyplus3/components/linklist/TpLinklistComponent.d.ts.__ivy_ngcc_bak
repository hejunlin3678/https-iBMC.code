import { EventEmitter } from '@angular/core';
import { TiBaseComponent } from '@cloud/tiny3';
import { TpLink } from '../crumb/TpCrumbModule';
export interface TpLinkOption {
    /**
     * 链接块的标题;
     */
    title?: string;
    /**
     * 设置链接是否为导航链接的默认样式（导航链接默认显示为蓝色）;
     * @default false
     */
    isNavigation?: boolean;
    /**
     * 设置链接块是否隐藏;
     * @default false
     */
    hide?: boolean;
    /**
     * 链接块的链接列表数据;
     */
    items?: Array<TpLinkItem>;
    /**
     * 允许有多余的属性字段
     */
    [propName: string]: any;
}
export interface TpLinkItem extends TpLink {
    /**
     * 设置链接是否隐藏;
     * @default false
     */
    hide?: boolean;
}
/**
 * 链接列表组件 linklist
 *
 * <example-url>../tinyplus3demo/#/linklist/linklist-all</example-url>
 */
export declare class TpLinklistComponent extends TiBaseComponent {
    /**
     * 组件数据
     */
    options: Array<TpLinkOption>;
    /**
     * 点击某个链接文本触发此事件
     */
    readonly select: EventEmitter<TpLinkItem>;
    /**
     * @ignore
     */
    onClick(item: TpLinkItem): boolean;
}
