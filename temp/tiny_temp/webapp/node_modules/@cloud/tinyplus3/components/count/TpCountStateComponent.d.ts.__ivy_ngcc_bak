import { SimpleChanges } from '@angular/core';
import { TiBaseComponent } from '@cloud/tiny3';
import { TpCountCode, TpCountDownStateLevel, TpCountInfos } from './TpCountComponent';
/**
 * 表格倒计时组件
 *
 * <example-url>../tinyplus3demo/#/count/count-all</example-url>
 */
export declare class TpCountStateComponent extends TiBaseComponent {
    /**
     * 倒计时类别，即倒计时countDownCode字段
     */
    countDownCode: TpCountCode;
    /**
     * 倒计时数据, 和2.21云服务倒计时信息查询接口返回的countDownInfos一致
     */
    countDownInfos: TpCountInfos;
    /**
     * @ignore
     * 是单独使用，还是和tp-count-time一起使用，单独使用没有逗号，一起使用有逗号
     */
    pattern: 'only' | 'combine';
    /**
     * @ignore
     * 资源状态描述文字
     */
    state: string;
    /**
     * @ignore
     * 文字提示的颜色等级
     */
    stateLevel: TpCountDownStateLevel;
    /**
     * @ignore
     * 分割号，分隔号放在资源状态中，主要解决资源状态和倒计时一起使用，文字中间有空间。
     */
    spliter: string;
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    private updateState;
    private isDayLess7;
}
