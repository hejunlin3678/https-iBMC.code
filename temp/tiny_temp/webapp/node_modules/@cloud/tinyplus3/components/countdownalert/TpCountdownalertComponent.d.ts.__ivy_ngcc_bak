import { TiBaseComponent } from '@cloud/tiny3';
import { TpCountDownCode, TpCountDownInfos } from '../countdown/TpCountdownModule';
/**
 * 倒计时提示组件
 *
 * <example-url>../tinyplus3demo/#/countdownalert/countdownalert-all</example-url>
 */
export declare class TpCountdownalertComponent extends TiBaseComponent {
    /**
     * 告警类型， 默认为error
     */
    type: 'success' | 'error' | 'warn' | 'prompt';
    /**
     * 是否显示左侧提示类型图标
     */
    typeIcon: boolean;
    /**
     * 是否显示关闭按钮
     */
    closeIcon: boolean;
    /**
     * 是否显示countdownalert
     */
    open: boolean;
    /**
     * 显示多长时间后消失，默认不延迟消失，只在点击关闭按钮时消失
     */
    dismissOnTimeout: number;
    /**
     * 是否触发下拉类组件消失的事件，适用于countdownalert的呈现状态影响页面下拉类组件位置的场景
     */
    triggerScroll: boolean;
    /**
     * 自定义告警内容
     */
    label: string;
    /**
     * 倒计时时间信息（未配置label时需要定义）
     */
    infos: TpCountDownInfos;
    /**
     * 资源状态（未配置label时需要定义）
     */
    status: TpCountDownCode;
    /**
     * 资源名称
     */
    resource: string;
    /**
     * 链接地址
     *
     * 包周期为续费时（status = hws_countdown_period_gracing | hws_countdown_period_frozen），服务需要传自定义续费链接；
     */
    linkHref: string;
    /**
     * 自定义客服联系电话号码
     * 10.1.2 版本新增
     */
    phoneNumber: string;
    /**
     * @ignore
     */
    innerLabel: string;
    /**
     * @ignore
     */
    showBuyLink: boolean;
    /**
     * @ignore
     */
    linkText: string;
    ngOnInit(): void;
    private getTimespan;
}
