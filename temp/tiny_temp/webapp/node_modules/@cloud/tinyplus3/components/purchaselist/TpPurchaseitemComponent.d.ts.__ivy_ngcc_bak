import { EventEmitter } from '@angular/core';
import { TiBaseComponent } from '@cloud/tiny3';
/**
 * <example-url>../tinyplus3demo/#/purchaselist/purchaselist-all</example-url>
 */
export declare class TpPurchaseitemComponent extends TiBaseComponent {
    /**
     * 商品信息是否带有region
     */
    hasArea: boolean;
    /**
     * 单个产品右侧叉号点击事件
     */
    readonly close: EventEmitter<any>;
    /**
     * @ignore
     */
    closeFn(): void;
}
