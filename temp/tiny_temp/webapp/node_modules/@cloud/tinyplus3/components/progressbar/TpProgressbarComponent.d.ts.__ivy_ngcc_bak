import { SimpleChanges } from '@angular/core';
import { TiBaseComponent } from '@cloud/tiny3';
/**
 * 进度条提示类型，percent-百分数 fraction-分数
 */
export declare type TpProgressbarLabelType = 'percent' | 'fraction';
/**
 * @ignore
 * 内部使用
 */
export declare type TpProgressbarWarnLevel = 'normal' | 'warn' | 'alarm';
/**
 *  进度条组件
 *
 *  本组件是ti-progressbar 的业务封装，有2种使用场景：
 *
 *  1.无预警：进度条[必选]+百分比或分数[必选]
 *
 *  2.有预警：进度条[必选]+百分比或分数[必选]
 *
 *  场景2设置了warn 和 alarm 接口后生效
 *
 *  10.0.1 新增
 *
 * <example-url>../tinyplus3demo/#/progressbar/progressbar-all</example-url>
 */
export declare class TpProgressbarComponent extends TiBaseComponent {
    /**
     * label 显示类型，
     */
    labelType: TpProgressbarLabelType;
    /**
     * 当前值
     */
    value: number;
    /**
     * 最大值
     */
    max: number;
    /**
     * 橙色（告警）的临界值,
     */
    warn: number;
    /**
     * 红色（告警）的临界值
     */
    alarm: number;
    /**
     * label单位
     */
    unit: string;
    /**
     * @ignore
     * 预警类型样式类别
     */
    warnLevel: TpProgressbarWarnLevel;
    /**
     * @ignore
     * 显示文字内容
     */
    label: string;
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    private updateWarnLevel;
    private updateLabel;
}
