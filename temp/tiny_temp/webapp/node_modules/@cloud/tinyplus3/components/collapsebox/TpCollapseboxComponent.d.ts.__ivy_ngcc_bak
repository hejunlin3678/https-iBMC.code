import { EventEmitter } from '@angular/core';
import { TiBaseComponent } from '@cloud/tiny3';
/**
 * 折叠框组件 collapsebox
 *
 * collapsebox组件是一个带有关闭操作的容器类组件，服务可自定义内容。
 *
 * <example-url>../tinyplus3demo/#/collapsebox/collapsebox-all</example-url>
 */
export declare class TpCollapseboxComponent extends TiBaseComponent {
    /**
     * @ignore
     * 设置折叠框默认展开
     */
    collapsed: boolean;
    /**
     * 是否可关闭
     * 10.0.3新增
     */
    closeable: boolean;
    /**
     * 关闭事件
     * 10.0.3新增
     */
    readonly close: EventEmitter<any>;
    /**
     * @ignore
     * 10.0.3新增
     */
    onClickClose(event: Event): void;
}
