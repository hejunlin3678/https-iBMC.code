    },
    {
        "work_name": "work.build_customize_package",
        "subworks": [
            {
                "work_name": "work.create_customize_efuse_pkg",
                "class_name": "Efusesupport",
                "path": "works.packet.work_create_customize_efuse_pkg"
            },
            {
                "work_name": "work.create_customize_efuse_partner_tcm_pkg",
                "class_name": "EfusePartnerTcmSupport",
                "path": "works.packet.work_create_customize_efuse_partner_tcm_pkg"
            },
            {
                "work_name": "work.build_forward_customize",
