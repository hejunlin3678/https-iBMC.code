        "path": "works.packet.work_create_ipmi_gdp_pkg",
        "wait": ["work.download_source"]
    },
    {
        "work_name": "work.build_mib_package",
        "class_name": "Mib",
        "path": "works.packet.work_create_mib_tosupport_pkg",
        "wait": ["work.download_source"]
    },
    {
        "work_name": "work.build_tc_mib_package",
        "class_name": "TC",
        "path": "works.packet.work_create_tcmib_tosupport_pkg",
        "wait": ["work.download_source"]
    },
    {
        "work_name": "work.build_firmware_K_package",
        "class_name": "FirmwareK",
        "path": "works.packet.create_firmware_k_pkg",
        "wait": ["work.download_source"]
    },
    {
        "work_name": "work.build_firmware_not_K_package",
        "class_name": "FirmwareNotK",
        "path": "works.packet.create_firmware_not_k_pkg",
        "wait": ["work.download_source"]
    },
    {
        "work_name": "work.build_firmware_revision_package",
        "class_name": "Firmware_Revision",
        "path": "works.packet.work_create_firmware_revision_gdp_pkg",
        "wait": ["work.download_source"]
    },
    {
        "work_name": "work.build_efuse_package",
