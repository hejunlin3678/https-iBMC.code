	<!-- PCIe Switch 温度表达式 -->
	<OBJECT NAME="PCIeSwTempExpr" CLASS="Expression" TYPE="Logical">
		<PROPERTY NAME="Value" TYPE="d">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="X" TYPE="rp">	<VALUE>sw_temp_monitor.MonSenValue</VALUE>	</PROPERTY>
		<PROPERTY NAME="Y" TYPE="y">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="Z" TYPE="y">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="Formula" TYPE="s">	<VALUE>(X &gt; 255) ? 25 : (((X &amp; 255) &gt;= 128) ? (-(255 - (X &amp; 255) + 1)) : (X &amp; 255))</VALUE>	</PROPERTY>
	</OBJECT>
