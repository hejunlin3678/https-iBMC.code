		<PROPERTY NAME="Value" TYPE="u">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="X" TYPE="u">	<VALUE>0x14000500</VALUE>	</PROPERTY>
		<PROPERTY NAME="Y" TYPE="rp">	<VALUE>SATADiskAnchor.Slot</VALUE>	</PROPERTY>
		<PROPERTY NAME="Z" TYPE="rp">	<VALUE>-&gt;HddBackCardAnchor.Slot</VALUE>	</PROPERTY>
