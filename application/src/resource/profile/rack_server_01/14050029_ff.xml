<?xml version='1.0' encoding='UTF-8'?>
<!-- 约束：文档编码格式必须是UTF-8 -->
<OBJECTS_CONFIG>
	<!-- DESCRIPTION项，用于注释对象。该项可以没有 -->
	<DESCRIPTION>配置文件描述信息</DESCRIPTION>
	<!-- 约束：OBJECT项的NAME属性和CLASS属性必须有 -->
	<!-- 本板的Anchor连接器描述 -->
	<OBJECT NAME="SecurityModuleAnchor" CLASS="Anchor" TYPE="Logical">
		<PROPERTY NAME="Legacy" TYPE="roa">	    <VALUE></VALUE>                      </PROPERTY>
		<PROPERTY NAME="Slot" TYPE="y">	<VALUE>0xff</VALUE>	</PROPERTY>
		<PROPERTY NAME="Position" TYPE="u">	<VALUE>0xffff</VALUE>	</PROPERTY>
		<PROPERTY NAME="SilkText" TYPE="s">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="Bus" TYPE="roa">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="Chip" TYPE="roa">	<VALUE></VALUE>	</PROPERTY>
	</OBJECT>
	<OBJECT NAME="SecurityModule" CLASS="SecurityModule" TYPE="Logical">
		<PROPERTY NAME="Presence" TYPE="y">	<VALUE>0x01</VALUE>	</PROPERTY>
		<PROPERTY NAME="SelfTestResult" TYPE="y">	<VALUE>0xff</VALUE>	</PROPERTY>
		<PROPERTY NAME="SpecificationType" TYPE="s">	<VALUE>TPM/TCM</VALUE>	</PROPERTY>
		<PROPERTY NAME="ManufacturerName" TYPE="s">	<VALUE>N/A</VALUE>	</PROPERTY>
		<PROPERTY NAME="SpecificationVersion" TYPE="s">	<VALUE>N/A</VALUE>	</PROPERTY>
		<PROPERTY NAME="ManufacturerVersion" TYPE="s">	<VALUE>N/A</VALUE>	</PROPERTY>
	</OBJECT>
	<OBJECT NAME="ComSecurityModule" CLASS="Component" TYPE="Logical">
		<PROPERTY NAME="FruId" TYPE="y">	<VALUE>0xff</VALUE>	</PROPERTY>
		<PROPERTY NAME="DeviceNum" TYPE="rp">	<VALUE>SecurityModuleAnchor.Slot</VALUE>	</PROPERTY>
		<PROPERTY NAME="DeviceType" TYPE="y">	<VALUE>0x22</VALUE>	</PROPERTY>
		<PROPERTY NAME="GroupID" TYPE="y">	<VALUE>0x01</VALUE>	</PROPERTY>
		<PROPERTY NAME="ContainerId" TYPE="y">	<VALUE>0x01</VALUE>	</PROPERTY>
		<PROPERTY NAME="Location" TYPE="s">	<VALUE>mainboard</VALUE>	</PROPERTY>
		<PROPERTY NAME="LocationObj" TYPE="ro">			<VALUE></VALUE></PROPERTY>
		<PROPERTY NAME="Function" TYPE="s">	<VALUE>Security Module</VALUE>	</PROPERTY>
		<PROPERTY NAME="DeviceName" TYPE="s">	<VALUE>Security Module</VALUE>	</PROPERTY>
		<PROPERTY NAME="ConfigType" TYPE="y">	<VALUE>0x01</VALUE>	</PROPERTY>
		<PROPERTY NAME="Presence" TYPE="y">	<VALUE>0x01</VALUE>	</PROPERTY>
		<PROPERTY NAME="Health" TYPE="y">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="BoardId" TYPE="q">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="VirtualFlag" TYPE="y">	<VALUE>1</VALUE>	</PROPERTY>
		<PROPERTY NAME="Manufacturer" TYPE="rp">	<VALUE>SecurityModule.ManufacturerName</VALUE>	</PROPERTY>
		<PROPERTY NAME="PowerState" TYPE="y">	<VALUE>1</VALUE>	</PROPERTY>
		<PROPERTY NAME="ComponentUniqueID" TYPE="s">  <VALUE>N/A</VALUE>  </PROPERTY>
		<PROPERTY NAME="HostId" TYPE="y">	<VALUE>0</VALUE>	</PROPERTY>
	</OBJECT>
	<!-- TPM自检失败 记录sel事件 -->
	<OBJECT NAME="SCYModuleSelfTestMntr" CLASS="EventMonitor" TYPE="Logical">
		<PROPERTY NAME="SubjectObj" TYPE="ro">	<VALUE>ComSecurityModule</VALUE>	</PROPERTY>
		<PROPERTY NAME="InformationObj" TYPE="ro">	<VALUE>::SecurityModuleSelfTestInfo</VALUE>	</PROPERTY>
		<PROPERTY NAME="PolicyObj" TYPE="ro">	<VALUE>::SecurityModuleSelfTestPlcy</VALUE>	</PROPERTY>
		<PROPERTY NAME="SensorObj" TYPE="ro">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="EventFRU" TYPE="ro">	<VALUE>ComSecurityModule</VALUE>	</PROPERTY>
		<PROPERTY NAME="Mode" TYPE="y">	<VALUE>1</VALUE>	</PROPERTY>
		<PROPERTY NAME="SavedMode" TYPE="y">	<VALUE>1</VALUE>	</PROPERTY>
		<PROPERTY NAME="Time" TYPE="u">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="State" TYPE="y">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="ScannedCount" TYPE="u">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="Reading" TYPE="rp">	<VALUE>SecurityModule.SelfTestResult</VALUE>	</PROPERTY>
		<PROPERTY NAME="SavedReading" TYPE="y">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="ReadingMask" TYPE="u">	<VALUE>0x00000001</VALUE>	</PROPERTY>
		<PROPERTY NAME="Threshold" TYPE="y">	<VALUE>0x00</VALUE>	</PROPERTY>
		<PROPERTY NAME="Hysteresis" TYPE="y">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="Operator" TYPE="y">	<VALUE>5</VALUE>	</PROPERTY>
		<PROPERTY NAME="AddInfo" TYPE="ay">	<VALUE></VALUE>	</PROPERTY>
		<PROPERTY NAME="DescriptionDynamic" TYPE="as">		    
		    <VALUE>SecurityModule.SpecificationType</VALUE>		
		</PROPERTY>
		<PROPERTY NAME="SuggestionDynamic" TYPE="as">	<VALUE></VALUE>	</PROPERTY>
	</OBJECT>
</OBJECTS_CONFIG>
