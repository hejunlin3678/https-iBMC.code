{
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_1_0.json",
    "title": "#HwKvmService.v1_0_0.HwKvmService",
    "$ref": "#/definitions/HwKvmService",
    "definitions": {
        "SetKvmKey": {
