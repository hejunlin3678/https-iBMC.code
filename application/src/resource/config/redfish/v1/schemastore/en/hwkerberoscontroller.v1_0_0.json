                "KerberosPort": {
                    "type": "number",
                    "minimum": 1,
                    "maximum": 65535,
                    "readonly": false,
                    "description": "Domain controller port number.",
                    "longDescription": "Domain controller port number configured by a user."
                },
                "Realm": {
                    "type": "string",
                    "readonly": false,
                    "description": "The name of the Kerberos realm in which the processor operates.",
                    "longDescription": "The name of the Kerberos realm in which the processor operates."
                },
                "KerberosGroups": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/KerberosGroups"
                    },
                    "readonly": false,
                    "description": "Kerberos user group. Multiple user groups can be created.",
                    "longDescription": "Kerberos user group information, including the user group ID, name, domain, role, login rules, and login interfaces."
