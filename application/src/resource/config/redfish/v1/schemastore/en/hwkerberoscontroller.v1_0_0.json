        "ImportKeyTable": {
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "title": {
                    "type": "string",
                    "readonly": true,
                    "description": "Friendly action name"
                },
                "target": {
                    "type": "string",
                    "format": "uri",
                    "readonly": true,
                    "description": "Link to invoke action"
                }
            },
            "description": "Action of importing a kerberos key table.",
            "longDescription": "Action of importing a kerberos key table."
        },
        "GroupLoginInterfaceItems": {
            "type": "string",
            "enum": [
                "Web",
                "Redfish"
            ],
            "enumDescriptions": {
                "Web": "Web-based login.",
                "Redfish": "Redfish-based login."
            },
            "readonly": false
        },
        "GroupLoginRuleItems": {
            "type": "string",
            "enum": [
                "Rule1",
                "Rule2",
                "Rule3"
            ],
            "enumDescriptions": {
                "Rule1": "First login rule.",
                "Rule2": "Second login rule.",
                "Rule3": "Third login rule."
            },
            "readonly": false
        },
        "GroupRole": {
            "type": "string",
            "enum": [
                "Common User",
                "Operator",
                "Administrator",
                "Custom Role 1",
                "Custom Role 2",
                "Custom Role 3",
                "Custom Role 4",
                "No Access"
            ],
            "enumDescriptions": {
                "Common User": "Ordinary user, built-in system.",
                "Operator": "Operator, built-in system.",
                "Administrator": "Administrator, built-in system, with the highest authority.",
                "Custom Role 1": "User-defined login user, which is subject to user customization.",
                "Custom Role 2": "User-defined login user, which is subject to user customization.",
                "Custom Role 3": "User-defined login user, which is subject to user customization.",
                "Custom Role 4": "User-defined login user, which is subject to user customization.",
                "No Access": "Users without any permissions."
            },
            "readonly": false
        },
