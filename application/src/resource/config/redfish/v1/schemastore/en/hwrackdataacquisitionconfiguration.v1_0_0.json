{
    "$schema": "http://redfish.dmtf.org/schemas/v1/redfish-schema.v1_1_0.json",
    "title": "#HwRackDataAcquisitionConfiguration.v1_0_0.HwRackDataAcquisitionConfiguration",
    "$ref": "#/definitions/HwRackDataAcquisitionConfiguration",
    "definitions": {
        "DeviceDataAcquisitionConfiguration": {
            "type": "object",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": true,
            "properties": {
                "SerialNumber": {
                    "type": "string",
                    "readonly": false,
                    "description": "The SN of the device within rack.",
                    "longDescription": "This property shall indicate a device with specific SN."
                },
                "AcquisitionItems": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "readonly": false,
                    "description": "The relative URI collection of taget resource.",
                    "longDescription": "This property shall contain all URI of the target need to GET."
                }
            }
        },
        "HwRackDataAcquisitionConfiguration": {
            "type": "object",
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "additionalProperties": false,
            "properties": {
                "@odata.context": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/context"
                },
                "@odata.id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/id"
                },
                "@odata.type": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/odata.4.0.0.json#/definitions/type"
                },
                "Oem": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Oem",
                    "description": "This is the manufacturer/provider specific extension moniker used to divide the Oem object into sections.",
                    "longDescription": "The value of this string shall be of the format for the reserved word *Oem*."
                },
                "Id": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Id"
                },
                "Description": {
                    "anyOf": [
                        {"$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Description"},
                        {"type": "null"}
                    ]
                },
                "Name": {
                    "$ref": "http://redfish.dmtf.org/schemas/v1/Resource.json#/definitions/Name"
                },
                "ConfigurationContent": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/DeviceDataAcquisitionConfiguration"
                    },
                    "readonly": false,
                    "description": "This array of objects enumerates all of the data acquisition configuration for devices within the rack.",
                    "longDescription": "The value of this property shall be an array of objects used to represent the data acquisition items for each device within the rack."
                }
            },
            "required": [
                "Id",
                "Name"
            ],
            "description": "Rack data acquisition configuration resource.",
            "longDescription": "The rack data acquisition configuration contains properties for managing data acquisition for device within rack."
        }
    },
