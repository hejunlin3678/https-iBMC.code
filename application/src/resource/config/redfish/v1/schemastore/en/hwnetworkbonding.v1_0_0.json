        },
        "LinkStatus": {
            "type": "string",
            "enum": [
                "LinkUp",
                "LinkDown"
            ],
            "enumDescriptions": {
                "LinkUp": "The link is available for communication on this interface.",
                "LinkDown": "There is no link on this interface, but the interface is connected."
            }
        },
        "MIILinkMonitoringFrequencyMS": {
            "type": "number",
            "minimum": 0,
            "maximum": 999999999
        }
