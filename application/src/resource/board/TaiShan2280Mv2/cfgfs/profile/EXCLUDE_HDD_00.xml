		<PROPERTY NAME="IsValid"  TYPE="rp">	<VALUE>JaPanExper.Value</VALUE>	</PROPERTY>
		<!-- 不归Raid卡管理的硬盘id -->
		<PROPERTY NAME="ExcludeHdd"  TYPE="ay">	
			<VALUE>0</VALUE><VALUE>1</VALUE><VALUE>2</VALUE><VALUE>3</VALUE><VALUE>4</VALUE><VALUE>5</VALUE>
			<VALUE>6</VALUE><VALUE>7</VALUE><VALUE>8</VALUE><VALUE>9</VALUE><VALUE>10</VALUE><VALUE>11</VALUE>
		</PROPERTY>
	</OBJECT> 
	<OBJECT NAME="2280M_DriveIdentify" CLASS="DriveIdentifyShield">
		<!-- 启动生效的条件，为 1 时启动，可以通过表达式来达成组合条件判断，例如某寄存器值数值为xx时 -->
		<PROPERTY NAME="IsValid"  TYPE="rp">	<VALUE>2280MExper.Value</VALUE>	</PROPERTY>
		<!-- 不归Raid卡管理的硬盘id -->
		<PROPERTY NAME="ExcludeHdd"  TYPE="ay">	
			<VALUE>42</VALUE><VALUE>43</VALUE>
		</PROPERTY>
	</OBJECT> 
