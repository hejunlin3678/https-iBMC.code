<?xml version='1.0' encoding='UTF-8'?>
<!-- 约束：文档编码格式必须是UTF-8 -->
<OBJECTS_CONFIG>
	<!-- DESCRIPTION项，用于注释对象。该项可以没有 -->
	<DESCRIPTION>配置文件描述信息</DESCRIPTION>
	<!-- 约束：OBJECT项的NAME属性和CLASS属性必须有 -->
	<!-- 时钟升级 -->
	<OBJECT NAME="clock_chip" CLASS="Chip" TYPE="Physical">
		<PROPERTY NAME="Addr" TYPE="u">	<VALUE>0xd2</VALUE>	</PROPERTY>
		<PROPERTY NAME="AddrWidth" TYPE="y">	<VALUE>1</VALUE>	</PROPERTY>
		<PROPERTY NAME="OffsetWidth" TYPE="y">	<VALUE>1</VALUE>	</PROPERTY>
		<PROPERTY NAME="PrevChip" TYPE="ro">	<VALUE>i2c4_pca9545_chip</VALUE>	</PROPERTY>
		<PROPERTY NAME="Index" TYPE="u">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="Level" TYPE="y">	<VALUE>1</VALUE>	</PROPERTY>
		<PROPERTY NAME="LBusType" TYPE="y">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="RBusType" TYPE="y">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="LBus" TYPE="ro">	<VALUE>pca9545_chan4</VALUE>	</PROPERTY>
	</OBJECT>
	<OBJECT NAME="clock_config_mgnt" CLASS="ChipConfigMgnt" TYPE="Logical">
		<PROPERTY NAME="RefChip" TYPE="ro">	<VALUE>clock_chip</VALUE>	</PROPERTY>
		<PROPERTY NAME="FirmwareFileName" TYPE="s">	<VALUE>/opt/pme/conf/vrd_mgnt/$.bin</VALUE>	</PROPERTY>
		<PROPERTY NAME="RemainingWrites" TYPE="q">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="ChipType" TYPE="u">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="UpdateFlag" TYPE="y">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="FirmwareVersion" TYPE="y">	<VALUE>0</VALUE>	</PROPERTY>
		<!-- 0x30 ASCII 对应版本 0 Au版本号寄存器暂未提供，先确保能升级-->
		<PROPERTY NAME="ExpectFirmwareVersion" TYPE="y">	<VALUE>0xff</VALUE>	</PROPERTY>
		<PROPERTY NAME="PrepareAction" TYPE="roa">	    <VALUE>PrepareUpgradeMode</VALUE></PROPERTY>
		<PROPERTY NAME="FinishAction" TYPE="roa">	    <VALUE>FinishUpgradeMode</VALUE></PROPERTY>
	</OBJECT>
	<!--Au的芯片,升级时需要bit7置1,升级完成后再置0-->
	<OBJECT NAME="UpgradeChannelAccessor" CLASS="Accessor" TYPE="Physical">
		<PROPERTY NAME="Chip" TYPE="ro">	<VALUE>::cpld1</VALUE>	</PROPERTY>
		<PROPERTY NAME="Type" TYPE="y">	<VALUE>0</VALUE>	</PROPERTY>
		<PROPERTY NAME="Size" TYPE="y">	<VALUE>1</VALUE>	</PROPERTY>
		<PROPERTY NAME="Mask" TYPE="u">	<VALUE>0x80</VALUE>	</PROPERTY>
		<PROPERTY NAME="Offset" TYPE="u">	<VALUE>0x3a4</VALUE>	</PROPERTY>
	</OBJECT>
	<OBJECT NAME="PrepareUpgradeMode" CLASS="Action" TYPE="Logical">
		<!-- 操作的Accessor -->
		<PROPERTY NAME="Destination" TYPE="y" RELATION_ACCESSOR="UpgradeChannelAccessor">	<VALUE>0</VALUE>	</PROPERTY>
		<!-- 写入的数据 -->
		<PROPERTY NAME="Data" TYPE="y">	<VALUE>0x01</VALUE>	</PROPERTY>
	</OBJECT>
	<OBJECT NAME="FinishUpgradeMode" CLASS="Action" TYPE="Logical">
		<!-- 操作的Accessor -->
		<PROPERTY NAME="Destination" TYPE="y" RELATION_ACCESSOR="UpgradeChannelAccessor">	<VALUE>0</VALUE>	</PROPERTY>
		<!-- 写入的数据 -->
		<PROPERTY NAME="Data" TYPE="y">	<VALUE>0x00</VALUE>	</PROPERTY>
	</OBJECT>
</OBJECTS_CONFIG>
