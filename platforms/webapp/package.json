{
  "name": "webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "start": "node ./node_modules/@angular/cli/bin/ng serve --ssl",
    "build": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configuration production && npm run post-build",
    "analyzer": "ng build --stats-json && webpack-bundle-analyzer ./dist/webapp/stats.json",
    "post-build": "npm run gzip && node post-build.config.js",
    "gzip": "gzipper compress ./dist/webapp/ --gzip-level 9"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.0",
    "@angular/common": "14.2.0",
    "@angular/compiler": "14.2.0",
    "@angular/core": "14.2.0",
    "@angular/forms": "14.2.0",
    "@angular/platform-browser": "14.2.0",
    "@angular/platform-browser-dynamic": "14.2.0",
    "@angular/router": "14.2.0",
    "@cloud/tiny3": "10.1.4",
    "@cloud/tinyplus3": "10.1.4",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "crypto-js": "4.1.1",
    "css-vars-ponyfill": "2.4.7",
    "echarts": "5.4.2",
    "ngx-echarts": "v8.0.1",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.0",
    "@angular-eslint/schematics": "^14.0.0-alpha.4",
    "@angular/cli": "14.2.0",
    "@angular/compiler-cli": "14.2.0",
    "@types/jasmine": "3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.20.5",
    "zone.js": "0.11.4",
    "codelyzer": "6.0.1",
    "gzipper": "7.1.0",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "protractor": "7.0.0",
    "resize-observer-polyfill": "1.5.1",
    "ts-node": "8.3.0",
    "tslint": "6.1.3",
    "typescript": "4.6.4",
    "webpack-bundle-analyzer": "4.6.1"
  }
}
